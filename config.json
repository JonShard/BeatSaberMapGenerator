{
    "generator": {
        "noteClusterTime": 0.01,
        "factories": {
            "maxAttempts": 1000,
            "randomFactory": {
                "enabled": false
            },
            "symmetricalFactory": {
                "enabled": true,
                "centerPointMode": true,
                "horizontalMode": true,
                "verticalMode": true,
                "allowOffsetPlane": true,
                "allowNotesInCenter": false
            },
            "markovFactory": {
                "enabled": true,
                "markovMatrixFilePath": "markovTransitionMatrix.data"
            }
        },
        "validators": {
            "validateTimeAfterNote": 2,
            "matrixValidator": {
                "enabled": true,
                "binaryMatrixFilePath": "binaryTransitionMatrix.data"
            },
            "doubleDownValidator": {
                "enabled": true,
                "angleToBeDoubleDown": 90
            },
            "adjacentValidator": {
                "enabled": true,
                "timeToBeAdjacent": 0.35,
                "timeToEnforceSameTrack": 0.20
            }
        }
    },
    "editor": {
        "autosaveEnabled": false,
        "windowWidth": 1600,
        "windowHeight": 900
    }
}